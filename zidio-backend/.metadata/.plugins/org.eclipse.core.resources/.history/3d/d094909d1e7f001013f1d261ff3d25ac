import com.nt.dto.SubscriptionDto;

@FeignClient(name="USERSUBSCRIPTIONSTATUS-SERVICE")
public interface UserSubscriptionStatusFindClient {
	@PostMapping("/api/usersubscriptionstatus/increment")
	public String findSubscription();
	
}

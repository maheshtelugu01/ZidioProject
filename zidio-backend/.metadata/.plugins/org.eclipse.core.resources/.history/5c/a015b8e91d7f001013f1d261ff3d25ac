package com.nt.client;

import org.springframework.cloud.openfeign.FeignClient;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PathVariable;

import com.nt.dto.SubscriptionDto;

@FeignClient(name="USERSUBSCRIPTIONSTATUS-SERVICE")
public interface UserSubscriptionStatusFindClient {
	@GetMapping("/api/subscription/plan/{id}")
	public SubscriptionDto findSubscription(@PathVariable Long id);
	
}
